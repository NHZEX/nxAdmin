{
    "name": "zxin/nxadmin",
    "description": "the new thinkphp 6.0 project",
    "type": "project",
    "keywords": [
        "framework",
        "thinkphp"
    ],
    "license": "Apache-2.0",
    "authors": [],
    "require": {
        "php": "^8.0",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-posix": "*",
        "ext-redis": "^4.3.0|^5.0",
        "ext-simplexml": "*",
        "ext-sockets": "*",
        "ext-zip": "*",
        "ext-zlib": "*",
        "composer-runtime-api": "^2",
        "composer/ca-bundle": "^1.2",
        "doctrine/annotations": "^1.10.2",
        "guzzlehttp/guzzle": "^7.0",
        "hashids/hashids": "^4.0",
        "league/flysystem": "^3.10",
        "nhzex/think-blade": "^5.0.0",
        "nhzex/think-phinx": "^2.0.1",
        "psr/cache": "^1.0",
        "rybakit/msgpack": "^0.9",
        "symfony/finder": "^5.4|^6.0",
        "symfony/polyfill-php81": "^1.22",
        "symfony/var-dumper": "^5.4|^6.0",
        "symfony/var-exporter": "^5.4|^6.0",
        "topthink/framework": "~6.1",
        "topthink/think-orm": "~2.0.54",
        "topthink/think-throttle": "^1.3.0",
        "zxin/think-auth": "^2.0",
        "zxin/think-captcha": "^1.0",
        "zxin/think-cors": "^2.1",
        "zxin/think-env": "^1.0.1",
        "zxin/think-redis": "^1.1",
        "zxin/think-validate": "^2.0",
        "zxin/utils": "^2.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpcompatibility/php-compatibility": "^9.3",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^8|^9",
        "rector/rector": "^0.14.8",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "replace": {
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*"
    },
    "conflict": {
        "topthink/think-orm": ">2.0.50 <=2.0.52"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app",
            "Util\\": "extend/Util",
            "Guzzle\\": "extend/Guzzle",
            "Tp\\": "extend/Tp"
        },
        "files": [
            "app/function.php",
            "app/constant.php",
            "app/status_code.php",
            "app/log_func.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php think service:discover",
            "@php think vendor:publish"
        ],
        "lint": [
            "php-cs-fixer fix -v"
        ],
        "phpstan-baseline": "phpstan --generate-baseline"
    },
    "extra": {
        "think-path": "thinkphp"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform-check": "php-only",
        "platform": {
        }
    },
    "repositories": [
        {
            "type":"composer",
            "url": "https://mirrors.aliyun.com/composer/",
            "canonical": false
        }
    ]
}
