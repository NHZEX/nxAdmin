{
    "name": "zxin/nxadmin",
    "description": "the new thinkphp 6.0 project",
    "type": "project",
    "keywords": [
        "framework",
        "thinkphp"
    ],
    "license": "Apache-2.0",
    "authors": [],
    "require": {
        "php": "^8.0",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-posix": "*",
        "ext-redis": "*",
        "ext-zlib": "*",
        "composer-runtime-api": "^2",
        "composer/ca-bundle": "^1.2",
        "guzzlehttp/guzzle": "^7.0",
        "hashids/hashids": "^4.0",
        "league/flysystem": "^3.10",
        "nhzex/think-blade": "~7.0.0-beta.2",
        "nhzex/think-phinx": "^4.0.3",
        "psr/cache": "^1|^2|^3",
        "rybakit/msgpack": "^0.9",
        "symfony/finder": "^6.0",
        "symfony/polyfill-php81": "^1.22",
        "symfony/var-dumper": "^6.0",
        "symfony/var-exporter": "^6.0",
        "topthink/framework": "^8.0.2",
        "topthink/think-orm": "~3.0.11",
        "topthink/think-throttle": "^2.0.0",
        "zxin/think-annotation": "^2.0.0",
        "zxin/think-auth": "^3.0.0",
        "zxin/think-captcha": "^2.0",
        "zxin/think-cors": "^3.0",
        "zxin/think-env": "^2.0",
        "zxin/think-redis": "^2.0.0",
        "zxin/think-route": "^2.0.0",
        "zxin/think-validate": "^3.0.0",
        "zxin/utils": "^2.2"
    },
    "require-dev": {
        "php-cs-fixer/shim": "^3.14",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^8|^9",
        "rector/rector": "~0.15"
    },
    "replace": {
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*"
    },
    "conflict": {
        "topthink/think-orm": "<2.0.50"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app",
            "Util\\": "extend/Util",
            "Guzzle\\": "extend/Guzzle",
            "Tp\\": "extend/Tp"
        },
        "files": [
            "app/function.php",
            "app/constant.php",
            "app/status_code.php",
            "app/log_func.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php think service:discover",
            "@php think vendor:publish"
        ],
        "lint": [
            "php-cs-fixer fix -v"
        ],
        "phpstan-baseline": "phpstan --generate-baseline"
    },
    "extra": {
        "think-path": "thinkphp"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform-check": "php-only",
        "platform": {
            "php": "8.0.2"
        }
    },
    "repositories": [
        {
            "type":"composer",
            "url": "https://mirrors.aliyun.com/composer/",
            "canonical": false
        }
    ]
}
