{
    "name": "nhzex/nxadmin",
    "description": "the new thinkphp framework",
    "type": "project",
    "keywords": [
        "framework",
        "thinkphp"
    ],
    "homepage": "http://thinkphp.cn/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "liu21st",
            "email": "liu21st@gmail.com"
        }
    ],
    "require": {
        "php": "~7.2.20 || ~7.3.8",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-gmp": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-posix": "*",
        "ext-redis": "^4.3.0 || ^5.0",
        "ext-simplexml": "*",
        "ext-sockets": "*",
        "ext-swoole": "^4.4.5",
        "ext-zip": "*",
        "ext-zlib": "*",
        "guzzlehttp/guzzle": "^6.3",
        "hashids/hashids": "^3.0",
        "matomo/ini": "^2.0",
        "nhzex/n-helper": "^1.0",
        "nhzex/think-blade": "^4.0.1",
        "nhzex/tp-swoole": "2.0.x-dev",
        "open-smf/connection-pool": "~1.0",
        "robmorgan/phinx": "^0.11",
        "rybakit/msgpack": "^0.5.4",
        "symfony/finder": "^4.3",
        "symfony/polyfill-php73": "^1.10",
        "symfony/var-dumper": "^4.2",
        "symfony/var-exporter": "^4.2",
        "topthink/framework": "6.0.x-dev#9744571904a008bff8567d5fc929ed161cad0f8b",
        "topthink/think-orm": "^2.0.23",
        "vlucas/phpdotenv": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "swoft/swoole-ide-helper": "~4.4.0"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app"
        },
        "psr-0": {
            "": "extend"
        },
        "files": [
            "app/constant.php",
            "app/status_code.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php think service:discover",
            "@php think vendor:publish"
        ]
    },
    "extra": {
        "think-path": "thinkphp"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": false,
        "platform": {
        }
    },
    "repositories": {
    }
}
