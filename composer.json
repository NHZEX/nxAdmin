{
    "name": "nhzex/nxadmin",
    "description": "the new thinkphp framework",
    "type": "project",
    "keywords": [
        "framework",
        "thinkphp"
    ],
    "homepage": "http://thinkphp.cn/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "liu21st",
            "email": "liu21st@gmail.com"
        }
    ],
    "require": {
        "php": "~7.2.10|~7.3.3",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-posix": "*",
        "ext-redis": "^4.3.0|^5.0",
        "ext-simplexml": "*",
        "ext-sockets": "*",
        "ext-zip": "*",
        "ext-zlib": "*",
        "doctrine/annotations": "^1.10.2",
        "guzzlehttp/guzzle": "^6.3",
        "hashids/hashids": "^3.0",
        "nhzex/n-helper": "^1.0",
        "nhzex/think-blade": "^4.2.0",
        "nhzex/think-cors": "^2.0",
        "nhzex/think-phinx": "^1.1.0",
        "rybakit/msgpack": "^0.7",
        "symfony/finder": "^4.2|^5.0",
        "symfony/polyfill-php73": "^1.10",
        "symfony/polyfill-php80": "^1.15",
        "symfony/var-dumper": "^4.2|^5.0",
        "symfony/var-exporter": "^4.2|^5.0",
        "topthink/framework": "~6.0.1",
        "topthink/think-orm": "~2.0.27"
    },
    "require-dev": {
        "phpunit/phpunit": "^8|^9"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app",
            "Captcha\\": "extend/Captcha",
            "Guzzle\\": "extend/Guzzle",
            "Tp\\": "extend/Tp"
        },
        "files": [
            "app/constant.php",
            "app/status_code.php",
            "app/reply_func.php",
            "app/log_func.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php think service:discover",
            "@php think vendor:publish"
        ]
    },
    "extra": {
        "think-path": "thinkphp"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../think-cors"
        }
    ]
}
