{
    "name": "nhzex/nxadmin",
    "description": "the new thinkphp framework",
    "type": "project",
    "keywords": [
        "framework",
        "thinkphp"
    ],
    "homepage": "http://thinkphp.cn/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "liu21st",
            "email": "liu21st@gmail.com"
        }
    ],
    "require": {
        "php": "~7.2.10",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-gmp": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-posix": "*",
        "ext-redis": "^4.3.0",
        "ext-simplexml": "*",
        "ext-zip": "*",
        "ext-zlib": "*",
        "guzzlehttp/guzzle": "^6.3",
        "hashids/hashids": "^3.0",
        "matomo/ini": "^2.0",
        "nhzex/n-helper": "dev-master",
        "nhzex/think-blade": "3.0.x-dev",
        "robmorgan/phinx": "^0.10.6",
        "rybakit/msgpack": "^0.5.4",
        "symfony/finder": "^4.3",
        "symfony/polyfill-php73": "^1.10",
        "symfony/var-dumper": "^4.2",
        "symfony/var-exporter": "^4.2",
        "topthink/framework": "6.0.x-dev#f0babbd6529a57d8043690d63aa9505ca8a2c60d",
        "topthink/think-orm": "2.0.x-dev#d0649a61896ddd060e7a038b79b3481d8730ceff",
        "topthink/think-view": "dev-master",
        "vlucas/phpdotenv": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app"
        },
        "psr-0": {
            "": "extend"
        },
        "files": [
            "app/constant.php",
            "app/status_code.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php think service:discover",
            "@php think vendor:publish"
        ]
    },
    "extra": {
        "think-path": "thinkphp"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.2.10"
        }
    },
    "repositories": {
    }
}
